$(function(){var form=$('#reserv_form');var formMessages=$('#res-messages');$(form).submit(function(event){event.preventDefault();var formData=$(form).serialize();$.ajax({type:'POST',url:$(form).attr('action'),data:formData}).done(function(response){$(formMessages).removeClass('alert-danger');$(formMessages).addClass('alert-success');$(formMessages).text(response);$('#res-name').val('');$('#res-email').val('');$('#res-phone').val('');$('#res-quantity').val('');}).fail(function(data){$(formMessages).removeClass('alert-success');$(formMessages).addClass('alert-danger');if(data.responseText!==''){$(formMessages).text(data.responseText);}else{$(formMessages).text('Oops! An error occured and your resgistration could not be complete.');}});});});